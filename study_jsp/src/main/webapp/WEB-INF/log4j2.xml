<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="DEBUC">
	<Appenders>
		<console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d - %msg%n"/>
		</console>
	</Appenders>
	<Loggers>
		<Root level="DEBUG">
			<AppenderRef ref="Console"/>
		</Root>
	</Loggers>	
</Configuration>